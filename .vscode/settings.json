{
  "cSpell.words": [
    "Axios",
    "consola",
    "Demi",
    "esno",
    "iframes",
    "iife",
    "Pausable",
    "Unref",
    "Unsubscribable",
    "vitepress",
    "vueuse"
  ],
  "prettier.enable": false,
  "grammarly.userWords": [
    "VueUse",
    "composables",
    "destructurable",
    "github",
    "isSupported",
    "monorepo",
    "prepublish",
    "vue"
  ],
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "json",
    "jsonc",
    "json5",
    "markdown"
  ],
  "vite.vitepress": true,
  "vite.vitepressBase": "packages",
  "vite.vitepressAutoRouting": true,
  "vite.buildCommand": "npm run docs:build",
  "typescript.tsdk": "node_modules/typescript/lib"
}
