{
  "name": "@vueuse/firebase",
  "type": "module",
  "version": "12.2.0",
  "description": "Enables realtime bindings for Firebase",
  "author": "Anthony Fu <https://github.com/antfu>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/vueuse/vueuse/tree/main/packages/firebase#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vueuse/vueuse.git",
    "directory": "packages/firebase"
  },
  "bugs": {
    "url": "https://github.com/vueuse/vueuse/issues"
  },
  "keywords": [
    "vue",
    "vue-use",
    "utils",
    "firebase",
    "vuefire"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./*": "./*",
    "./useAuth": {
      "import": "./useAuth.mjs",
      "require": "./useAuth.cjs"
    },
    "./useFirestore": {
      "import": "./useFirestore.mjs",
      "require": "./useFirestore.cjs"
    },
    "./useRTDB": {
      "import": "./useRTDB.mjs",
      "require": "./useRTDB.cjs"
    }
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "unpkg": "./index.iife.min.js",
  "jsdelivr": "./index.iife.min.js",
  "types": "./index.d.ts",
  "files": [
    "**/*.cjs",
    "**/*.d.cts",
    "**/*.d.mts",
    "**/*.d.ts",
    "**/*.js",
    "**/*.mjs"
  ],
  "scripts": {
    "build": "rollup --config=rollup.config.ts --configPlugin=rollup-plugin-esbuild",
    "prepack": "pnpm run build"
  },
  "peerDependencies": {
    "firebase": ">=9.0.0"
  },
  "dependencies": {
    "@vueuse/shared": "workspace:*",
    "vue": "catalog:"
  }
}
