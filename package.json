{
  "name": "@vueuse/monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "4.0.0-beta.26",
  "description": "Collection of essential Vue Composition API",
  "scripts": {
    "build": "esno ./scripts/build.ts",
    "build:types": "tsc --emitDeclarationOnly && npm run types:fix",
    "release": "esno ./scripts/release.ts && git push --follow-tags",
    "publish:ci": "esno ./scripts/publish.ts",
    "gen": "run-p gen:*",
    "gen:import": "esno ./scripts/import.ts",
    "gen:readme": "esno ./scripts/update-readme.ts",
    "gen:package": "esno ./scripts/update-package.ts",
    "types:fix": " esno ./scripts/types-fix.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint './{packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md}'",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf dist types typings storybook-static packages/*/dist",
    "dev": "start-storybook -p 6006",
    "sb": "start-storybook -p 6006",
    "sb:build": "npm run build:types && build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md} *.js": [
      "eslint --fix"
    ]
  },
  "author": "Anthony Fu<https://github.com/antfu>",
  "license": "MIT",
  "dependencies": {
    "@vue/composition-api": "^1.0.0-beta.18",
    "vue-demi": "^0.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.4.3",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^6.0.0",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^5.3.21",
    "@storybook/theming": "^5.3.21",
    "@storybook/vue": "^5.3.21",
    "@types/fs-extra": "^9.0.2",
    "@types/jest": "^26.0.15",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "~14.14.0",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/semver": "^7.3.4",
    "@vue/babel-preset-jsx": "^1.2.3",
    "@vue/runtime-dom": "^3.0.2",
    "@vue/test-utils": "^1.1.0",
    "autoprefixer": "^9.8.6",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-preset-vue": "^2.0.2",
    "commander": "^6.1.0",
    "consola": "^2.15.0",
    "conventional-github-releaser": "^3.1.5",
    "dayjs": "^1.9.3",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-markdown": "^1.0.2",
    "esm": "^3.2.25",
    "esno": "^0.2.4",
    "fast-glob": "^3.2.4",
    "firebase": "^7.24.0",
    "fs-extra": "^9.0.1",
    "github-markdown-css": "^4.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.4.2",
    "markdown-it": "^12.0.1",
    "markdown-it-highlight-lines": "^1.0.2",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-loader": "^0.7.0",
    "markdown-it-prism": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.35",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.2",
    "prismjs": "^1.22.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.1",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tailwindcss": "^1.9.5",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "version-bump-prompt": "^6.1.0",
    "vue": "^2.6.12",
    "vue-i18n": "^8.22.1",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "vue-tsx-support": "^2.3.3"
  }
}
