{
  "name": "@vueuse/core",
  "version": "0.0.16",
  "description": "Collection of essential Vue Composition API",
  "scripts": {
    "build": "node ./scripts/build",
    "release:gh": "npm test && bump --commit --tag --push --preid alpha && conventional-github-releaser -p angular",
    "release:local": "npm test && npm run readme:update && bump --commit --tag --preid alpha && npm run publich:ci",
    "publich:ci": "node ./scripts/publish.js all",
    "switch": "node ./scripts/switch",
    "import": "node ./scripts/import",
    "typings": "tsc --emitDeclarationOnly",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint {packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md}",
    "lint:fix": "npm run lint -- --fix",
    "readme:update": "node ./scripts/updateReadme",
    "clear": "rimraf dist typings storybook-static",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "npm run switch 2 && npm run typings && build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md} *.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vueuse.git"
  },
  "keywords": [
    "vue",
    "vue-use",
    "utils"
  ],
  "author": "Anthony Fu<https://github.com/antfu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antfu/vueuse/issues"
  },
  "homepage": "https://github.com/antfu/vueuse#readme",
  "dependencies": {},
  "peerDependencies": {
    "vue": "^2.6.11",
    "@vue/composition-api": "^0.3.4",
    "@vue/runtime-dom": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "0.2.10",
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@rollup/plugin-typescript": "2.1.0",
    "@storybook/addon-actions": "5.3.7",
    "@storybook/addon-knobs": "5.3.7",
    "@storybook/addon-links": "5.3.7",
    "@storybook/addon-notes": "5.3.7",
    "@storybook/addons": "5.3.7",
    "@storybook/theming": "5.3.7",
    "@storybook/vue": "5.3.7",
    "@types/fs-extra": "8.0.1",
    "@types/inquirer": "6.5.0",
    "@types/jest": "24.9.0",
    "@types/js-yaml": "3.12.1",
    "@types/node": "13.1.8",
    "@types/semver": "6.2.0",
    "@vue/babel-preset-jsx": "1.1.2",
    "@vue/composition-api": "0.3.4",
    "@vue/runtime-dom": "3.0.0-alpha.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "autoprefixer": "9.7.4",
    "axios": "0.19.1",
    "babel-loader": "8.0.6",
    "babel-preset-vue": "2.0.2",
    "commander": "4.1.0",
    "consola": "2.11.3",
    "eslint": "6.8.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-markdown": "1.0.1",
    "firebase": "7.7.0",
    "fs-extra": "8.1.0",
    "github-markdown-css": "3.0.1",
    "husky": "4.0.10",
    "inquirer": "7.0.3",
    "jest": "24.9.0",
    "js-yaml": "3.13.1",
    "lint-staged": "9.5.0",
    "markdown-it": "10.0.0",
    "markdown-it-highlight-lines": "1.0.2",
    "markdown-it-link-attributes": "3.0.0",
    "markdown-it-loader": "0.7.0",
    "markdown-it-prism": "2.0.3",
    "postcss": "7.0.26",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "prismjs": "1.19.0",
    "raw-loader": "4.0.0",
    "rimraf": "3.0.0",
    "rollup": "1.29.1",
    "rollup-plugin-dts": "1.2.0",
    "rollup-plugin-uglify": "6.0.4",
    "tailwindcss": "1.1.4",
    "ts-jest": "24.3.0",
    "typescript": "3.7.5",
    "version-bump-prompt": "5.0.7",
    "vue": "2.6.11",
    "vue-i18n": "8.15.3",
    "vue-loader": "15.8.3",
    "vue-template-compiler": "2.6.11",
    "vue-tsx-support": "2.3.3"
  }
}
