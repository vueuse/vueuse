{
  "name": "@vueuse/monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "version": "4.0.0",
  "description": "Collection of essential Vue Composition Utilities",
  "scripts": {
    "build": "esno ./scripts/build.ts",
    "build:rollup": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly && npm run types:fix",
    "release": "esno ./scripts/release.ts && git push --follow-tags",
    "publish:ci": "esno ./scripts/publish.ts",
    "prepare": "esno ./scripts/update.ts",
    "size": " esno ./scripts/export-size.ts",
    "types:fix": " esno ./scripts/types-fix.ts",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:2",
    "test:2": "vue-demi-switch 2 vue2 && jest",
    "test:3": "vue-demi-switch 3 && jest",
    "test:all": "npm run test:3 && npm run test:2",
    "test:watch": "jest --watch",
    "lint": "eslint './{packages,scripts}/**/*.{js,ts,tsx,vue,md}'",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf dist types typings packages/*/dist",
    "dev": "npm run docs",
    "docs": "npm run docs:prepare && vitepress dev packages",
    "docs:prepare": "esno ./scripts/docs.ts",
    "docs:build": "npm run docs:prepare && cross-env NODE_ENV=production npm run build:types && vitepress build packages"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md} *.js": [
      "eslint --fix"
    ]
  },
  "author": "Anthony Fu<https://github.com/antfu>",
  "license": "MIT",
  "dependencies": {
    "vue-demi": "latest"
  },
  "optionalDependencies": {
    "@vue/composition-api": "^1.0.0-beta.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.4.3",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/fs-extra": "^9.0.5",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^3.12.5",
    "@types/markdown-table": "^2.0.0",
    "@types/node": "~14.14.14",
    "@types/semver": "^7.3.4",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/composition-api": "^1.0.0-beta.21",
    "@vue/runtime-dom": "^3.0.4",
    "@vue/test-utils": "^1.1.2",
    "autoprefixer": "^9.8.6",
    "axios": "^0.20.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-preset-vue": "^2.0.2",
    "consola": "^2.15.0",
    "conventional-github-releaser": "^3.1.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.9.7",
    "esbuild-register": "^1.1.1",
    "eslint": "^7.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-markdown": "^1.0.2",
    "esm": "^3.2.25",
    "esno": "^0.3.0",
    "export-size": "^0.4.0",
    "fast-glob": "^3.2.4",
    "firebase": "^7.24.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "js-yaml": "^3.14.1",
    "lint-staged": "^10.5.3",
    "markdown-table": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.35",
    "postcss-import": "^12.0.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-dts": "^1.4.14",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tailwindcss": "^1.9.6",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "version-bump-prompt": "^6.1.0",
    "vue": "^3",
    "vue-i18n": "^8.22.2",
    "vue2": "npm:vue@2"
  }
}
