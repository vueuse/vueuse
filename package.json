{
  "name": "@vueuse/monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "4.0.8",
  "description": "Collection of essential Vue Composition Utilities",
  "scripts": {
    "build": "esno scripts/build.ts",
    "build:rollup": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly && nr types:fix",
    "build:redirects": "esno scripts/redirects.ts",
    "release": "esno scripts/release.ts && git push --follow-tags",
    "publish:ci": "esno scripts/publish.ts",
    "update": "esno scripts/update.ts",
    "update:full": "nr build:types && esno scripts/update.ts",
    "size": " esno scripts/export-size.ts",
    "types:fix": " esno scripts/fix-types.ts",
    "typecheck": "tsc --noEmit",
    "test": "nr test:2",
    "test:2": "vue-demi-switch 2 vue2 && jest",
    "test:3": "vue-demi-switch 3 && jest",
    "test:all": "nr test:3 && nr test:2",
    "test:watch": "jest --watch",
    "lint": "eslint './{packages,scripts}/**/*.{js,ts,tsx,vue,md}'",
    "lint:fix": "nr lint -- --fix",
    "clean": "rimraf dist types packages/*/dist",
    "dev": "nr docs",
    "docs": "vitepress dev packages",
    "docs:build": "nr update && vitepress build packages && nr build:redirects && esno scripts/post-docs.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ],
    "*.js":[
        "eslint --fix"
    ]
  },
  "author": "Anthony Fu<https://github.com/antfu>",
  "license": "MIT",
  "dependencies": {
    "vue-demi": "^0.6.0"
  },
  "optionalDependencies": {
    "@vue/composition-api": "^1.0.0-beta.26"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.4.3",
    "@antfu/ni": "^0.4.1",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@iconify/json": "^1.1.286",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/fs-extra": "^9.0.6",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/markdown-table": "^2.0.0",
    "@types/node": "~14.14.20",
    "@types/semver": "^7.3.4",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/composition-api": "^1.0.0-beta.26",
    "@vue/runtime-dom": "^3.0.5",
    "@vue/test-utils": "^1.1.2",
    "autoprefixer": "^10.2.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-preset-vue": "^2.0.2",
    "consola": "^2.15.0",
    "conventional-github-releaser": "^3.1.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.3",
    "esbuild-register": "^1.2.1",
    "eslint": "^7.17.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-markdown": "^1.0.2",
    "esm": "^3.2.25",
    "esno": "^0.4.0",
    "export-size": "^0.4.0",
    "fast-glob": "^3.2.4",
    "firebase": "^8.2.2",
    "fs-extra": "^9.0.1",
    "gray-matter": "^4.0.2",
    "husky": "4.3.7",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.3",
    "markdown-table": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.1",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tailwindcss": "^2.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "version-bump-prompt": "^6.1.0",
    "vite-plugin-components": "^0.6.6",
    "vite-plugin-editor-nav": "^0.0.2",
    "vite-plugin-icons": "^0.2.1",
    "vite-plugin-pwa": "^0.3.6",
    "vitepress": "npm:@knightly/vitepress@pr203",
    "vue": "^3.0.5",
    "vue-chemistry": "^0.1.4",
    "vue2": "npm:vue@2"
  }
}
