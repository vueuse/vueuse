{
  "name": "@vueuse/core",
  "version": "0.0.0-alpha.7",
  "description": "Collection of essential Vue Composition API",
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "scripts": {
    "build": "node ./build/build.js",
    "release": "npm run lint && bump --commit --preid alpha && node ./build/publish.js all",
    "switch": "node ./build/switch",
    "test": "jest",
    "lint": "eslint {src,.storybook,build}/**/*.{js,ts,tsx,vue}",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run switch 2 && build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,.storybook,build}/**/*.{js,ts,tsx,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vueuse.git"
  },
  "keywords": [
    "vue",
    "vue-use",
    "utils"
  ],
  "author": "Anthony Fu<https://github.com/antfu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antfu/vueuse/issues"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/antfu/vueuse#readme",
  "dependencies": {},
  "peerDependencies": {
    "vue": "^2.6.11",
    "@vue/composition-api": "^0.3.4",
    "@vue/runtime-dom": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "0.2.10",
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-typescript": "7.7.7",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-knobs": "5.2.8",
    "@storybook/addon-links": "5.2.8",
    "@storybook/addon-notes": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/theming": "5.2.8",
    "@storybook/vue": "5.2.8",
    "@types/fs-extra": "^8.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "24.0.24",
    "@types/node": "13.1.0",
    "@types/semver": "^6.2.0",
    "@vue/babel-preset-jsx": "1.1.2",
    "@vue/composition-api": "0.3.4",
    "@vue/runtime-dom": "^3.0.0-alpha.0",
    "@vue/test-utils": "1.0.0-beta.30",
    "babel-loader": "8.0.6",
    "babel-preset-vue": "2.0.2",
    "commander": "^4.0.1",
    "consola": "^2.11.1",
    "eslint": "6.8.0",
    "eslint-plugin-jest": "23.1.1",
    "fs-extra": "^8.1.0",
    "github-markdown-css": "3.0.1",
    "husky": "3.1.0",
    "inquirer": "^7.0.1",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "markdown-it": "10.0.0",
    "markdown-it-highlight-lines": "1.0.2",
    "markdown-it-link-attributes": "3.0.0",
    "markdown-it-loader": "0.7.0",
    "markdown-it-prism": "2.0.3",
    "prismjs": "1.17.1",
    "rimraf": "3.0.0",
    "semver": "^7.1.1",
    "ts-jest": "24.2.0",
    "typescript": "3.7.4",
    "version-bump-prompt": "5.0.6",
    "vue": "2.6.11",
    "vue-loader": "15.8.3",
    "vue-template-compiler": "2.6.11",
    "vue-tsx-support": "2.3.2"
  }
}
